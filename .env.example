# Convex - Get from your Convex dashboard
NEXT_PUBLIC_CONVEX_URL=https://your-deployment.convex.cloud

# GitHub OAuth - Create an OAuth App at https://github.com/settings/developers
# Set the callback URL to: http://localhost:3000/api/auth/callback (for local dev)
NEXT_PUBLIC_GITHUB_CLIENT_ID=your_client_id
GITHUB_CLIENT_SECRET=your_client_secret

# UploadThing - Get from https://uploadthing.com/dashboard
UPLOADTHING_TOKEN=your_uploadthing_token
